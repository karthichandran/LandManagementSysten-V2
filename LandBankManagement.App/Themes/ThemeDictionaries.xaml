<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="DefaultBackground" Color="White" />
            <SolidColorBrush x:Key="LightBackground" Color="#7CBEE0" />
            <SolidColorBrush x:Key="BorderBrush" Color="#FFD3D3D3" />
            <SolidColorBrush x:Key="ListBackGround" Color="#2F7BAD"></SolidColorBrush>
            <SolidColorBrush x:Key="ListHeaderBackGround" Color="Beige"/>
            <SolidColorBrush x:Key="SectionBackGround" Color="#CC4D11"/>
            <SolidColorBrush x:Key="TaskBarBackGround" Color="#CC4D11"/>
            <SolidColorBrush x:Key="ToolBarButtonForeGround" Color="#805BBF"/>
            <SolidColorBrush x:Key="DetailsBackGround" Color="#EECEFF"></SolidColorBrush>
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="#7CBEE0" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="DefaultBackground" Color="#333" />
            <SolidColorBrush x:Key="LightBackground" Color="#444" />
            <SolidColorBrush x:Key="BorderBrush" Color="#555" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
